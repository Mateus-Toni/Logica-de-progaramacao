algoritmo "semnome"
var
    n1,n2,n3:inteiro
inicio
      escreval("número de início")
      leia(n1)
      escreval("número final")
      leia(n2)
      escreval("incremento")
      leia(n3)
      enquanto n1<=n2 faca
               escreval(n1)
               n1 <- n1 +n3
      fimenquanto
fimalgoritmo