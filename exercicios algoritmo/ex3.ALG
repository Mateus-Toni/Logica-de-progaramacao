algoritmo "semnome"
var
    nome:caracter
    sal:real
inicio
      escreval("======================")
      escreval("     empresa xyz     ")
      escreval("Qual seu nome? ")
      leia(nome)
      escreval("Digite o seu salário: ")
      leia(sal)
      escreval("======================")
      escreval("o funcionario ",nome, ", recebera no mes de setembro um salario de ",sal)
fimalgoritmo