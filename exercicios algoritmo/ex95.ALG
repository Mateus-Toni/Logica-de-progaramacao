algoritmo "semnome"

funcao soma(a,b:inteiro):inteiro
inicio
      retorne(a+b)
fimfuncao
var
   x,y,somat:inteiro
inicio
      escreval("Digite dois valores a serem somados: ")
      leia(x)
      escreval("+")
      leia(y)
      escreval("---")
      somat <- soma(x,y)
      escreval(somat)
fimalgoritmo