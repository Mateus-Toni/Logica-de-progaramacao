algoritmo "semnome"
var
   num,num2:inteiro
procedimento soma(a,b:inteiro)
var
somas:inteiro
inicio
      somas <- a+b
      escreval(somas)
fimprocedimento
inicio
      escreval("Primeiro valor da soma: ")
      leia(num)
      escreval("segundo valor da soma: ")
      leia(num2)
      soma(num,num2)
      
fimalgoritmo