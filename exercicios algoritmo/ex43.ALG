algoritmo "semnome"
var
     c:inteiro
inicio
      c <- 30
      enquanto c >= 0 faca
               se (c%4=0) entao
                  escreva("[")
                  escreval(c,"]")
               senao
                    escreval(c)
               fimse
               c <- c -1
      fimenquanto

fimalgoritmo